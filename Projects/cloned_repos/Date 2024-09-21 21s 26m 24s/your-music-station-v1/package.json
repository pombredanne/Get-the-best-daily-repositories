{
  "name": "your-music-station",
  "version": "1.0.3",
  "main": "./dist/electron/main/main.js",
  "author": "kikolove",
  "license": "MIT",
  "scripts": {
    "dev": "node .electron-vite/dev-runner.js",
    "build": "cross-env BUILD_TARGET=clean node .electron-vite/build.js && node .electron-vite/build.js && electron-builder -c build.json",
    "build:win32": "cross-env BUILD_TARGET=clean node .electron-vite/build.js && node .electron-vite/build.js && electron-builder -c build.json --win  --ia32",
    "build:win64": "cross-env BUILD_TARGET=clean node .electron-vite/build.js && node .electron-vite/build.js && electron-builder -c build.json --win  --x64",
    "build:mac": "cross-env BUILD_TARGET=clean node .electron-vite/build.js && node .electron-vite/build.js && electron-builder -c build.json --mac",
    "build:dir": "cross-env BUILD_TARGET=clean node .electron-vite/build.js && node .electron-vite/build.js && electron-builder -c build.json --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vite/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vite/build.js",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "ant-design-vue": "2.2.8",
    "antd-theme-generator": "^1.2.10",
    "axios": "^0.22.0",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "glob": "^7.2.0",
    "howler": "^2.2.3",
    "rgbaster": "^2.1.1",
    "vue-request": "^1.2.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.10.3",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "adm-zip": "^0.5.9",
    "cfonts": "^2.10.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.2.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "javascript-obfuscator": "^2.19.0",
    "less": "^4.1.2",
    "multispinner": "^0.2.1",
    "portfinder": "^1.0.28",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-obfuscator": "^0.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "2.6.5",
    "vue": "3.2.22",
    "vue-router": "^4.0.11"
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
