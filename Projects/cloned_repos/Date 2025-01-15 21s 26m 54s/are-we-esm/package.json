{
  "name": "are-we-esm",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@9.15.4",
  "description": "CLI to check your project's ESM support status",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/are-we-esm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/are-we-esm.git"
  },
  "bugs": "https://github.com/antfu/are-we-esm/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "cac": "^6.7.14",
    "cli-progress": "^3.12.0",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.2",
    "tinyexec": "^0.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@antfu/ni": "^23.2.0",
    "@antfu/utils": "^8.1.0",
    "@pnpm/list": "^1000.0.5",
    "@pnpm/reviewing.dependencies-hierarchy": "^1001.0.3",
    "@pnpm/types": "^1000.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.6",
    "@types/picomatch": "^3.0.1",
    "bumpp": "^9.10.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "lint-staged": "^15.3.0",
    "pkg-types": "^1.3.1",
    "pnpm": "^9.15.4",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
