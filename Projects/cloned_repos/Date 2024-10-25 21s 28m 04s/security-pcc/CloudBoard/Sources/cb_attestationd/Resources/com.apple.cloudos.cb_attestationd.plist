<plist version="1.0">
<dict>
	<key>_PersistToBootMode</key>
	<string>rem</string>
	<key>Label</key>
	<string>com.apple.cloudos.cb_attestationd</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/libexec/cb_attestationd</string>
	</array>
	<key>MachServices</key>
	<dict>
		<key>com.apple.cloudos.cb_attestationd</key>
		<true/>
	</dict>
	<key>ProcessType</key>
	<string>Interactive</string>
	<key>RunAtLoad</key>
	<true/>
	<key>EnablePressuredExit</key>
	<false/>
	<key>KeepAlive</key>
	<dict>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	<key>UserName</key>
	<string>mobile</string>
</dict>
</plist>
